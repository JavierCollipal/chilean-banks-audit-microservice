{
  "name": "chilean-banks-audit",
  "version": "1.7.1",
  "description": "Educational microservice for auditing Chilean bank login security features (cybersecurity course)",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "bin": {
    "chilean-banks-audit": "dist/main.js"
  },
  "files": [
    "dist",
    "README.md",
    "ETHICAL-USAGE-GUIDE.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "audit:bank": "ts-node src/scripts/audit-bank.ts",
    "audit:all": "ts-node src/scripts/run-full-audit.ts",
    "audit:demo": "ts-node src/scripts/puppeteer-demo.ts",
    "save:vulnerabilities": "ts-node src/scripts/save-vulnerabilities.ts",
    "seed:banks": "ts-node src/scripts/seed-banks.ts",
    "add:banks": "ts-node src/scripts/add-new-banks.ts"
  },
  "keywords": [
    "cybersecurity",
    "banking",
    "audit",
    "education",
    "nestjs",
    "puppeteer",
    "security-research",
    "chilean-banks",
    "security-audit",
    "defensive-security",
    "ethical-hacking",
    "web-security",
    "ssl-analysis",
    "security-headers",
    "csrf-protection",
    "penetration-testing"
  ],
  "author": "Neko-Arc System",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JavierCollipal/chilean-banks-audit-microservice.git"
  },
  "homepage": "https://github.com/JavierCollipal/chilean-banks-audit-microservice#readme",
  "bugs": {
    "url": "https://github.com/JavierCollipal/chilean-banks-audit-microservice/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^10.4.20",
    "axios": "^1.6.5",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.1",
    "dotenv": "^16.4.1",
    "helmet": "^7.1.0",
    "mongodb": "^6.3.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "puppeteer": "^21.9.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.6",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.spec.ts",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/scripts/**",
      "!main.ts",
      "!**/*.module.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  }
}
